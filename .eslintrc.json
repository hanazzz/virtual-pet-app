{
    "extends": "airbnb",
    "rules": {
        "react/jsx-no-undef": ["error", { "allowGlobals": true }],
        "no-else-return": ["error", { "allowElseIf": true}],
        "object-curly-newline": [2, {
            "multiline": true,
            "consistent": true
        }]
    },
    "env": {
        "browser": true
    },
    "globals": {
        "React": "readonly",
        "ReactDOM": "readonly",
        "PropTypes": "readonly",
        "Navbar": "readonly",
        "Footer": "readonly",
        "SelectInput": "readonly",
        "CustomPetCreator": "readonly",
        "SpeciesToggle": "readonly",
        "PetGenerator": "readonly",
        "Settings": "readonly",
        "Logout": "readonly",
        "DeleteObj": "readonly",
        "RenamePet": "readonly",
        "CurrentPet": "readonly",
        "PetAttributes": "readonly",
        "WeatherDisplay": "readonly",
        "StatDisplay": "readonly",
        "Stat": "readonly",
        "Login": "readonly",
        "CreateAcct": "readonly",
        "Play": "readonly",
        "Button": "readonly",
        "Modal": "readonly",
        "ModalBtn": "readonly",
        "ModalBox": "readonly",
        "ModalFooter": "readonly",
        "AttributesModal": "readonly",
        "Interaction": "readonly",
        "Feed": "readonly",
        "AdoptPetBtn": "readonly",
        "GeneratePetBtn": "readonly",
        "PetHeading": "readonly"
    }   
}