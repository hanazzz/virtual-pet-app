{
    "extends": "airbnb",
    "rules": {
        "react/jsx-no-undef": ["error", { "allowGlobals": true }],
        "no-else-return": ["error", { "allowElseIf": true}],
        "object-curly-newline": [2, {
            "multiline": true,
            "consistent": true
        }],
        "react/jsx-no-bind": ["off"]
    },
    "env": {
        "browser": true
    },
    "globals": {
        "React": "readonly",
        "ReactDOM": "readonly",
        "ReactQuery": "readonly",
        "PropTypes": "readonly",
        "Navbar": "readonly",
        "Footer": "readonly",
        "Heading1": "readonly",
        "Heading2": "readonly",
        "Heading3": "readonly",
        "Card": "readonly",
        "SelectInput": "readonly",
        "Form": "readonly",
        "CustomPetCreator": "readonly",
        "SpeciesToggle": "readonly",
        "PetGenerator": "readonly",
        "Settings": "readonly",
        "ToggleTempUnit": "readonly",
        "Logout": "readonly",
        "Alert": "readonly",
        "DeleteObj": "readonly",
        "RenamePet": "readonly",
        "CurrentPet": "readonly",
        "PetAttributes": "readonly",
        "WeatherDisplay": "readonly",
        "StatDisplay": "readonly",
        "Stat": "readonly",
        "Mood": "readonly",
        "Login": "readonly",
        "CreateAcct": "readonly",
        "Play": "readonly",
        "Button": "readonly",
        "Modal": "readonly",
        "ModalBtn": "readonly",
        "ModalBox": "readonly",
        "ModalTitle": "readonly",
        "ModalFooter": "readonly",
        "About": "readonly",
        "AttributesModal": "readonly",
        "Interaction": "readonly",
        "Feed": "readonly",
        "PotentialPet": "readonly",
        "AdoptPet": "readonly",
        "AdoptPetBtn": "readonly",
        "GeneratePetBtn": "readonly",
        "usePetData": "readonly",
        "useMakeCustomImg": "readonly",
        "PetHeading": "readonly"
    }   
}