{
    "extends": "airbnb",
    "rules": {
        "react/jsx-no-undef": ["error", { "allowGlobals": true }],
        "no-else-return": ["error", { "allowElseIf": true}],
        "object-curly-newline": [2, {
            "multiline": true,
            "consistent": true
        }]
    },
    "env": {
        "browser": true
    },
    "globals": {
        "React": "readonly",
        "ReactDOM": "readonly",
        "PropTypes": "readonly",
        "Navbar": "readonly",
        "Footer": "readonly",
        "PetGenerator": "readonly",
        "DeletePet": "readonly",
        "CurrentPet": "readonly",
        "PetDisplay": "readonly",
        "WeatherDisplay": "readonly",
        "StatDisplay": "readonly",
        "Stat": "readonly",
        "Login": "readonly",
        "CreateAcct": "readonly",
        "Play": "readonly",
        "Modal": "readonly",
        "Attributes": "readonly",
        "Interaction": "readonly",
        "Feed": "readonly"
    }   
}